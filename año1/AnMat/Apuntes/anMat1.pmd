$cos^2(t) + sen^2(t) = 1$

\subsubsection{Diferencia seno y coseno}

$cos\left(t-\displaystyle \frac{\pi}{2}\right) = sen(t)$

$sen\left(t+\displaystyle \frac{\pi}{2}\right) = cos(t)$

\subsubsection{Invertir signo}

$cos(t+\pi) = -cos(t)$

$sen(t+\pi) = -sen(t)$

\subsubsection{Suma de angulos: Coseno}

$cos(t+s) = cos(t)cos(s)-sen(t)sen(s)$

$t = s$ $\Rightarrow$ $cos(2t) = cos^2(t)-sen^2(t)$

\subsubsection{Suma de angulos: Seno}

$sen(t+s) = sen(t)cos(s)+sen(s)cos(t)$

$t = s$ $\Rightarrow$ $sen(2t) = 2sen(t)cos(t)$

