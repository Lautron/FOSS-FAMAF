& \textbf{fun} factorial(n: \textbf{nat}) \textbf{ret} f : nat\\
& \quad f := 1\\
& \quad \textbf{for} i := 2 \textbf{to} n \textbf{do}\\
& \quad \quad f := f * n\\
& \quad \textbf{od}\\
& \textbf{end fun}\\
\end{tabular}

