& \textbf{fun} factorial(n: \textbf{nat}) \textbf{ret} f : nat\\
& \quad if n = 0 then\\
& \quad \quad f := 1\\
& \quad else\\
& \quad \quad f := n * factorial(n-1)\\
& \quad fi\\
& \textbf{end fun}\\
\end{tabular}

